{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "VITE_API_BASE_URL": "@vite_api_base_url",
    "VITE_GOOGLE_MAPS_API_KEY": "@vite_google_maps_api_key"
  },
  "build": {
    "env": {
      "VITE_API_BASE_URL": "@vite_api_base_url",
      "VITE_GOOGLE_MAPS_API_KEY": "@vite_google_maps_api_key"
    }
  }
}
